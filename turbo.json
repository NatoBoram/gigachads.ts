{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"docker": {},
		"docker:build": {},
		"docker:kill": {},
		"docker:run": {},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"clean": {},
		"dev": {
			"dependsOn": ["^build"]
		},
		"format": {},
		"lint": {
			"dependsOn": ["^build"]
		},
		"lint:fix": {
			"dependsOn": ["^build"]
		},
		"start": {
			"dependsOn": ["build"]
		},
		"test": {
			"dependsOn": ["^build"]
		},
		"test:coverage": {
			"dependsOn": ["^build"]
		},
		"test:watch": {
			"dependsOn": ["^build"],
			"cache": false
		}
	}
}
